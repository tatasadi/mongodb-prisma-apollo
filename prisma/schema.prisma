datasource db{
  provider = "mongodb"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model sessions {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    title String
    description String?
    startsAt String?
    endsAt String?
    room String?
    day String?
    format String?
    track String?
    level String?
  }